!cpu 6502
!to "build/PRELAUNCH.INDEXED/CHUCK.YEAGER",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $840
         sta   $865
         jsr   $800       ; decompress
         jsr   DisableAccelerator
         jsr   $BE00
         jsr   $843       ; decompress more
         +LC_REBOOT
         jmp   $6004

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
