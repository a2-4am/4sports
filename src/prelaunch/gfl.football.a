!cpu 6502
!to "build/PRELAUNCH.INDEXED/GFL.FOOTBALL",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $9F3
         jsr   $800       ; load "DOS"
         lda   #<callback
         sta   $B748
         lda   #>callback
         sta   $B749
         jmp   $B700

callback
         ldx   #0
         stx   $9E52
         inx
         stx   $9E53
         lda   #<callback2
         sta   $B7B8
         lda   #>callback2
         sta   $B7B9
         +DISABLE_ACCEL
         jmp   $9D84

callback2
         jsr   $BD00
         lda   $FC4
         eor   #$1B
         bne   +
         lda   $FC5
         bne   +
         sta   $100B      ; reset vector
+        rts

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
