!cpu 6502
!to "build/PRELAUNCH.INDEXED/INTL.GRAN.PRIX",plain
*=$106

         !source "src/macros.a"

         lda   #$60       ; RTS instead of JMP
         sta   $201A
         jsr   $2000      ; decompress

         lda   #0
         sta   $6010
         lda   #1
         sta   $6015      ; reset vector patch

         jmp   $6000

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
