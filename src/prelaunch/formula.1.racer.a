!cpu 6502
!to "build/PRELAUNCH.INDEXED/FORMULA.1.RACER",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $818
         jsr   $800

         lda   #$2C       ; reset vector patch
         sta   $71D0
         stx   $71D5
         sta   $71DA

         +DISABLE_ACCEL
         jmp   $7000

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
