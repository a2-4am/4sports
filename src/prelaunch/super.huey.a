!cpu 6502
!to "build/PRELAUNCH.INDEXED/SUPER.HUEY",plain
*=$106

         !source "src/macros.a"

         +READ_RAM2_WRITE_RAM2
         jsr   EnableAccelerator
         +LC_REBOOT
         +READ_RAM2_NO_WRITE
         lda   #$60
         sta   $3972
         sta   $849C
         jsr   $3931      ; decompress
         +DISABLE_ACCEL
         jsr   HideLaunchArtwork
         jsr   $800
         +ENABLE_ACCEL
         jsr   $8473      ; decompress
         +DISABLE_ACCEL
         jmp   $800

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
