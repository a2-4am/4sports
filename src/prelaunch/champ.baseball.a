!cpu 6502
!to "build/PRELAUNCH.INDEXED/BASEBALL",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $41DF
         jsr   $4000      ; unpack
         inc   $3f4       ; reboot

         +READ_RAM2_NO_WRITE
         jsr   DisableAccelerator
         +READ_RAM1_WRITE_RAM1

         jmp   $864

!if * > HideLaunchArtwork {
  !error "code is too large, ends at ", *
}
