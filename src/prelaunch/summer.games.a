!cpu 6502
!to "build/PRELAUNCH.INDEXED/SUMMER.GAMES",plain
*=$106

         !source "src/macros.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $9EB
         jsr   $800      ; decompress

         +READ_RAM1_WRITE_RAM1
         +LC_REBOOT
         +DISABLE_ACCEL_AND_HIDE_ARTWORK
         jmp   $B700

!if * > PrelaunchMax {
  !error "code is too large, ends at ", *
}
