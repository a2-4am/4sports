!cpu 6502
!to "build/PRELAUNCH.INDEXED/FAMILY.FEUD",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL

         lda   #$60
         sta   $A06
         jsr   $800       ; load "DOS"
         lda   #<callback
         sta   $B748
         lda   #>callback
         sta   $B749
         jmp   $B700

callback
         sta   $9E37
         lda   #<callback2
         sta   $B7B8
         lda   #>callback2
         sta   $B7B9
         +DISABLE_ACCEL
         jmp   $9D84

callback2
         jsr   $BD00
         lda   $B7EC
         eor   #$13
         bne   +
         lda   $B7ED
         eor   #$0E
         bne   +
         sta   $960F      ; reset vector
+        rts

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
